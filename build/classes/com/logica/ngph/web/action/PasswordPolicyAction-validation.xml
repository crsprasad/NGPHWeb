<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE validators PUBLIC 
                "-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
                "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="minChars">
		<field-validator type="regex">
			<param name="fieldName">minChars</param>
			<param name="expression"><![CDATA[[0-9]]]></param>
			<message> Minimum Password Length is Not Valid </message>
		</field-validator>
	</field>
	
	<field name="maxChars">
		<field-validator type="regex">
			<param name="fieldName">maxChars</param>
			<param name="expression"><![CDATA[[0-9]]]></param>
			<message> Maximum Password Length is Not Valid </message>
		</field-validator>
	</field>
	
	<field name="noofDigits">
		<field-validator type="regex">
			<param name="fieldName">noofDigits</param>
			<param name="expression"><![CDATA[[0-9]]]></param>
			<message> No of Digits is Not Valid </message>
		</field-validator>
	</field>
	
	<field name="noofSpecialChars">
		<field-validator type="regex">
			<param name="fieldName">noofSpecialChars</param>
			<param name="expression"><![CDATA[[0-9]]]></param>
			<message> No of Special Character is Not Valid </message>
		</field-validator>
	</field>
	
	<field name="noofUpperChars">
		<field-validator type="regex">
			<param name="fieldName">noofUpperChars</param>
			<param name="expression"><![CDATA[[0-9]]]></param>
			<message> No of Upper Case is Not Valid </message>
		</field-validator>
	</field>
	
	<field name="noofLowerChars">
		<field-validator type="regex">
			<param name="fieldName">noofLowerChars</param>
			<param name="expression"><![CDATA[[0-9]]]></param>
			<message> No of Lower Case is Not Valid </message>
		</field-validator>
	</field>
	
	<field name="passwordExpDays">
		<field-validator type="regex">
			<param name="fieldName">passwordExpDays</param>
			<param name="expression"><![CDATA[[0-9]]]></param>
			<message> Password Expires in (days) is Not Valid </message>
		</field-validator>
	</field>
	
	<field name="maxNoofPassChangesDay">
		<field-validator type="regex">
			<param name="fieldName">maxNoofPassChangesDay</param>
			<param name="expression"><![CDATA[[0-9]]]></param>
			<message> No of Password changes in a day is Not Valid </message>
		</field-validator>
	</field>
	
</validators>	
